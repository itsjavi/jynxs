    "jsx-renderer",
    "tsx",
    "tsx-runtime",
    "tsx-renderer"
  ],
  "homepage": "https://github.com/itsjavi/jynxs#readme",
  "bugs": {
    "url": "https://github.com/itsjavi/jynxs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itsjavi/jynxs.git"
  },
  "license": "MIT",
  "author": "Javi Aguilar",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
